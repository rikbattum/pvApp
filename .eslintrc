{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      1,
      "windows"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "browser": 1,
    "jasmine": 2
  },
  "globals": {
    "angular": 1

  },
  "jasmine": true,
  "plugins": [
    "jasmine"
  ]

}